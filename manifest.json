{
    "manifest_version": 2,
    "name": "ChromeX",
    "version": "1.0",
    "description": "dwadwa",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "./main.js"
            ]
        }
    ],

    "browser_action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },

    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    }
}